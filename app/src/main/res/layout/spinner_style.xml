<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/groupStylewhite_A700cornerRadius"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="start">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/txtLabelSpinner"
            style="@style/txtInterregular14"
            android:textColor="@color/black"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="@string/lbl_tanggal_awal" />

        <ImageView
            android:id="@+id/imageArrowdown1"
            android:layout_width="@dimen/_14pxh"
            android:layout_height="@dimen/_8pxh"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_gravity="center"
            android:scaleType="fitXY"
            android:src="@drawable/img_arrowdown_gray_900"
            tools:ignore="ContentDescription" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>