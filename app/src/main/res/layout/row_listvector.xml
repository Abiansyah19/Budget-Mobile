<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:id="@+id/linearColumnvector"
        style="@style/groupStylegray_300cornerRadius"
        android:layout_width="@dimen/_150pxh"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:layout_marginStart="@dimen/_10pxh"
        android:layout_marginEnd="@dimen/_10pxh"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/btnVector"
            android:layout_width="@dimen/_48pxh"
            android:layout_height="@dimen/_48pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:background="@drawable/rectangle_bg_bluegray_600_radius_20"
            android:contentDescription="tes"
            android:minWidth="48dp"
            android:minHeight="48dp"
            android:paddingHorizontal="@dimen/_11pxh"
            android:paddingVertical="@dimen/_11pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_vector"
            tools:ignore="ContentDescription" />

        <TextView
            android:id="@+id/txtPrice"
            style="@style/txtInterbold17"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_49pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:ellipsize="end"
            android:lineSpacingExtra="@dimen/_4pxh"
            android:singleLine="true"
            android:text="Rp 3000"
            tools:text="@string/lbl_1_800_00" />

        <TextView
            android:id="@+id/txtIncome"
            style="@style/txtInterregular14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginBottom="@dimen/_16pxv"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="Rp 5000"
            tools:text="@string/lbl_income" />
    </LinearLayout>
</layout>
